{
    "name": "xemlock/twig-coffee",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xemlock",
            "email": "xemlock@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "twig/twig": "^1.24",
        "coffeescript/coffeescript": "master"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "suggest": {
        "patchwork/jsqueeze": "Minification of compiled JavaScript code using JSqueeze",
        "tedivm/jshrink": "Minification of compiled JavaScript code using JShrink",
        "mrclay/minify": "Minification of compiled JavaScript code using JSMinPlus"
    },
    "autoload": {
        "psr-0": {
            "TwigCoffee_": "library/"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/phpcoffee/coffeescript"
        }
    ]
}
